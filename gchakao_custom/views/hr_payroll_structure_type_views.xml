<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="hr_payroll_structure_type_inherit_form" model="ir.ui.view">
            <field name="name">hr.payroll.structure.type.inherit.form</field>
            <field name="model">hr.payroll.structure.type</field>
            <field name="inherit_id" ref="hr_payroll.hr_payroll_structure_type_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='wage_type']" position="after">
                    <field name="code"/>
                    <field name="is_commission"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>